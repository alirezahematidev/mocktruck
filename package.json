{
  "name": "truck",
  "version": "0.0.1",
  "description": "Locally mock CRUD apis",
  "main": "lib/main.js",
  "module": "lib/main.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "type": "module",
  "scripts": {
    "cli": "yarn build:dev && ts-node-esm  bin/index.mjs",
    "cli:invoke": "ts-node cli/index.mts",
    "build:dev": "rimraf lib && yarn compile  && webpack --mode=development --config webpack.config.cjs",
    "build:prod": "rimraf lib && yarn compile && webpack --mode=production --node-env=production --config webpack.config.cjs",
    "watch": "webpack --watch",
    "compile": "tsc -b",
    "test": "yarn compile && jest",
    "truck:serve": "node src/services/index.ts"
  },
  "files": [
    "bin",
    "lib"
  ],
  "keywords": [
    "mock",
    "api",
    "server",
    "crud"
  ],
  "author": "Alireza Hemati",
  "license": "MIT",
  "bin": {
    "truck": "bin/index.mjs"
  },
